{
	"align": {
		"prefix": "ali",
		"body": [
		  "\\begin{align*}",
		  "$0",
		  "\\end{align*}"
		],
		"description": "align"
	},

	"begin": {
		"prefix": "begin",
		"body": [
			"\\begin{$1}",
			"$0", 
			"\\end{${1}}"
		],
		"description": "\\begin{}â€¦\\end{}",
	},

	"curly braces": {
		"prefix": "curl",
		"body": [
		  "\\{ $0 \\}"
		],
		"description": "curly braces"
	},

	"definition": {
		"prefix": "def",
		"body": [
		  "\\begin{definition}[$0]",
		  "\\end{definition}"
		],
		"description": "definition"
	  },

	"enumeration": {
		"prefix": "enu",
		"body": [
		  "\\begin{enumerate}",
		  "  \\item",
		  "\\end{enumerate}"
		],
		"description": "enumeration"
	},

	"equation": {
		"prefix": "eq",
		"body": [
			"\\begin{equation*}",
			"$0",
			"\\end{equation*}"
		],
		"description": "Equation",
	},

	"fraction": {
			"prefix": "frac",
			"body": [
			  "\\frac{$1}{$0}"
			],
			"description": "fraction"
	},

	"greater than": {
		"prefix": "gtr",
		"body": [
		  "$>$"
		],
		"description": "greater than"
	},

	"itemize": {
		"prefix": "ite",
		"body": [
		  "\\begin{itemize}",
		  "  \\item",
		  "\\end{itemize}"
		],
		"description": "itemize"
	},

	"less than": {
		"prefix": "les",
		"body": [
		  "$<$"
		],
		"description": "less than"
	},

	"math mode":{
		"prefix": "usd",
		"body": "\\$ $1 \\$ $0",
		"description": "math mode"
	},	

	"section": {
		"prefix": "sec",
		"body": [
		  "\\section{$0}"
		],
		"description": "section"
	},

	"small caps": {
			"prefix": "sc",
			"body": [
				"\\textsc{$1}"
			],
			"description": "adds small caps"
	},
			
	"signature": {
				"prefix": "sign",
				"body": [
				  "Regards,",
				  "Anjali"
				],
				"description": "signature"
	},

	"split": {
		"prefix": "spli",
		"body": [
		  "\\begin{equation*}",
		  "\\begin{split}",
		  "$0",
		  "\\end{split}",
		  "\\end{equation*}"
		],
		"description": "split"
	},

	"subscript": {
		"prefix": "subs",
		"body": [
		  "$1_{$0}"
		],
		"description": "subscript"
	},

	"superscript": {
		"prefix": "sups",
		"body": [
		  "$1^{$0}"
		],
		"description": "superscript"
	},
	
	"summation": {
			"prefix": "sum",
			"body": [
			  "\\sum_{$1}^{$0}"
			],
			"description": "summation"
	},

	"theorem": {
			"prefix": "thm",
			"body": [
			  "\\begin{theorem}",
			  "\\end{theorem}"
			],
			"description": "theorem"
	},

	"x and y": {
		"prefix": "xy",
		"body": [
		  "\\$X\\$ and \\$Y\\$"
		],
		"description": "x and y"
	}

}